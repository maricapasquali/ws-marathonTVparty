@prefix : <http://www.semanticweb.org/marica/ontologies/marathontvparty/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix fo: <http://purl.org/foodontology#> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix event: <http://purl.org/NET/c4dm/event.owl#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix mtparty: <http://www.semanticweb.org/marica/ontologies/marathontvparty/> .
@base <http://www.semanticweb.org/marica/ontologies/marathontvparty/> .

<http://www.semanticweb.org/marica/ontologies/marathontvparty> rdf:type owl:Ontology ;
                                                                owl:versionIRI <http://www.semanticweb.org/marica/ontologies/marathontvparty/1.0.0> ;
                                                                owl:imports <http://purl.org/foodontology> ,
                                                                            <http://purl.org/NET/c4dm/event.owl> ;
                                                                dc:creator "Marica Pasquali" ;
                                                                dc:description "This ontology was developed with the intention of modeling \"knowledge\" for organizing events involving movie or TV series marathons." ;
                                                                dc:title "Marathon TV Party" ;
                                                                dcterms:modified "2023-05-26" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/marica/ontologies/marathontvparty/ratingInteger
mtparty:ratingInteger rdf:type rdfs:Datatype ;
                      owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                            owl:onDatatype xsd:positiveInteger ;
                                            owl:withRestrictions ( [ xsd:maxInclusive "10"^^xsd:positiveInteger
                                                                   ]
                                                                 )
                                          ] .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/NET/c4dm/event.owl#place
event:place owl:equivalentProperty mtparty:physicalPlace .


###  http://purl.org/NET/c4dm/event.owl#sub_event
event:sub_event owl:inverseOf mtparty:isContinuationOf .


###  http://purl.org/NET/c4dm/event.owl#time
event:time owl:equivalentProperty mtparty:startsTVPartyAt .


###  http://schema.org/actor
schema:actor rdf:type owl:ObjectProperty .


###  http://schema.org/address
schema:address rdf:type owl:ObjectProperty .


###  http://schema.org/creator
schema:creator rdf:type owl:ObjectProperty .


###  http://schema.org/director
schema:director rdf:type owl:ObjectProperty .


###  http://schema.org/partOfSeason
schema:partOfSeason rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty .


###  http://schema.org/partOfSeries
schema:partOfSeries rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/areProjectedOn
mtparty:areProjectedOn rdf:type owl:ObjectProperty ;
                       rdfs:domain mtparty:TVPartyShowSequence ;
                       rdfs:range [ rdf:type owl:Class ;
                                    owl:unionOf ( mtparty:Device
                                                  mtparty:ProjectorWithScreen
                                                )
                                  ] .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/bringsFood
mtparty:bringsFood rdf:type owl:ObjectProperty ;
                   owl:inverseOf mtparty:isBroughtFoodBy ;
                   rdfs:domain mtparty:PrivateParticipant ;
                   rdfs:range mtparty:HomeFood .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/buysFood
mtparty:buysFood rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf gr:seeks ;
                 owl:inverseOf mtparty:isBoughtFoodBy ;
                 rdfs:domain mtparty:Participant ;
                 rdfs:range mtparty:FoodOffering ;
                 owl:propertyChainAxiom ( mtparty:buysFoods
                                          rdfs:member
                                        ) .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/buysFoodAt
mtparty:buysFoodAt rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf time:hasTime ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain mtparty:FoodCart .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/buysFoods
mtparty:buysFoods rdf:type owl:ObjectProperty ;
                  owl:inverseOf mtparty:isFoodCartOf ;
                  rdfs:domain mtparty:Participant ;
                  rdfs:range mtparty:FoodCart .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/buysTicket
mtparty:buysTicket rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf gr:seeks ;
                   owl:inverseOf mtparty:isBoughtTicketBy ;
                   rdf:type owl:InverseFunctionalProperty ;
                   rdfs:domain foaf:Person ;
                   rdfs:range mtparty:BuyerTicketOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/buysTicketAt
mtparty:buysTicketAt rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf time:hasTime ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain mtparty:TicketCart .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/buysTickets
mtparty:buysTickets rdf:type owl:ObjectProperty ;
                    owl:inverseOf mtparty:isTicketCartOf ;
                    rdf:type owl:InverseFunctionalProperty ;
                    rdfs:domain foaf:Person ;
                    rdfs:range mtparty:TicketCart .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/capacity
mtparty:capacity rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( schema:Place
                                             schema:VirtualLocation
                                           )
                             ] ;
                 rdfs:range gr:QuantitativeValue .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/correspondsToSellerTicketOffering
mtparty:correspondsToSellerTicketOffering rdf:type owl:ObjectProperty ,
                                                   owl:FunctionalProperty ;
                                          rdfs:domain mtparty:BuyerTicketOffering ;
                                          rdfs:range mtparty:SellerTicketOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/endsShowAt
mtparty:endsShowAt rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf time:hasTime ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain mtparty:TVPartyShowSequenceItem .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/foodMenu
mtparty:foodMenu rdf:type owl:ObjectProperty ;
                 owl:inverseOf mtparty:isFoodMenuOf ;
                 rdfs:domain mtparty:PublicPhysicalTVParty ;
                 rdfs:range mtparty:FoodMenu .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/food_in
mtparty:food_in rdf:type owl:ObjectProperty ;
                owl:inverseOf mtparty:hasFoodOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/foodseller
mtparty:foodseller rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf mtparty:seller ;
                   owl:inverseOf mtparty:foodseller_in ;
                   rdfs:domain mtparty:PublicPhysicalTVParty ;
                   rdfs:range mtparty:PublicFoodSeller .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/foodseller_in
mtparty:foodseller_in rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasBudget
mtparty:hasBudget rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf event:factor ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain mtparty:TVParty ;
                  rdfs:range gr:PriceSpecification .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasFoodCategory
mtparty:hasFoodCategory rdf:type owl:ObjectProperty ;
                        rdfs:domain fo:Food ;
                        rdfs:range mtparty:FoodCategory .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasFoodOffering
mtparty:hasFoodOffering rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( mtparty:FoodCartItem
                                                    mtparty:FoodVariantOffering
                                                  )
                                    ] ;
                        rdfs:range mtparty:FoodOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasFoodPlusOffering
mtparty:hasFoodPlusOffering rdf:type owl:ObjectProperty ;
                            rdfs:domain mtparty:FoodOffering ;
                            rdfs:range mtparty:FoodPlusOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasFoodQuantity
mtparty:hasFoodQuantity rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain mtparty:FoodCartItem ;
                        rdfs:range gr:QuantitativeValue .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasFoods
mtparty:hasFoods rdf:type owl:ObjectProperty ;
                 rdfs:domain [ owl:intersectionOf ( mtparty:PrivateTVParty
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty mtparty:physicalPlace ;
                                                      owl:someValuesFrom schema:Residence
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( mtparty:FoodBag
                                            mtparty:FoodCart
                                          )
                            ] .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasShow
mtparty:hasShow rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain mtparty:TVPartyShowSequenceItem ;
                rdfs:range mtparty:Show .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasStatus
mtparty:hasStatus rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain mtparty:TVParty ;
                  rdfs:range mtparty:TVPartyStatus .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/hasTicketModel
mtparty:hasTicketModel rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf gr:hasMakeAndModel ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain mtparty:Ticket ;
                       rdfs:range mtparty:TicketModel .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/includesFood
mtparty:includesFood rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf gr:includes ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain mtparty:FoodOffering ;
                     rdfs:range mtparty:TakeawayFood .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/includesFoodPlus
mtparty:includesFoodPlus rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf gr:includes ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain mtparty:FoodPlusOffering ;
                         rdfs:range mtparty:FoodPlus .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/includesTicket
mtparty:includesTicket rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf gr:includes ;
                       rdf:type owl:FunctionalProperty ,
                                owl:InverseFunctionalProperty ;
                       rdfs:domain mtparty:BuyerTicketOffering ;
                       rdfs:range mtparty:Ticket ;
                       owl:propertyDisjointWith mtparty:includesTicketModel .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/includesTicketModel
mtparty:includesTicketModel rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf gr:includes ;
                            owl:inverseOf mtparty:isTicketModelIncludedIn ;
                            rdfs:domain mtparty:SellerTicketOffering ;
                            rdfs:range mtparty:TicketModel .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isBoughtFoodBy
mtparty:isBoughtFoodBy rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isBoughtTicketBy
mtparty:isBoughtTicketBy rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isBroughtFoodBy
mtparty:isBroughtFoodBy rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isContinuationOf
mtparty:isContinuationOf rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ,
                                  owl:IrreflexiveProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isFoodCartOf
mtparty:isFoodCartOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isFoodMenuOf
mtparty:isFoodMenuOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isMemberOf
mtparty:isMemberOf rdf:type owl:ObjectProperty ;
                   owl:inverseOf rdfs:member .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isReactionOf
mtparty:isReactionOf rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain mtparty:Reaction ;
                     rdfs:range mtparty:Participant .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isResidenceOf
mtparty:isResidenceOf rdf:type owl:ObjectProperty ;
                      rdfs:domain schema:Residence ;
                      rdfs:range mtparty:PrivateParticipant .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isSoldFoodBy
mtparty:isSoldFoodBy rdf:type owl:ObjectProperty ;
                     owl:inverseOf mtparty:sellsFood .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isSoldTicketBy
mtparty:isSoldTicketBy rdf:type owl:ObjectProperty ;
                       owl:inverseOf mtparty:sellsTicket .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isTicketCartOf
mtparty:isTicketCartOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/isTicketModelIncludedIn
mtparty:isTicketModelIncludedIn rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/location
mtparty:location rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain mtparty:TVParty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/organizer
mtparty:organizer rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf event:agent ;
                  owl:inverseOf mtparty:organizer_in ;
                  rdfs:domain mtparty:TVParty ;
                  rdfs:range mtparty:Organizer .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/organizer_in
mtparty:organizer_in rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/participant
mtparty:participant rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf event:agent ;
                    owl:inverseOf mtparty:participant_in ;
                    rdfs:domain mtparty:TVParty ;
                    rdfs:range mtparty:Participant ;
                    owl:propertyChainAxiom ( mtparty:participantWithTicket
                                             mtparty:pwt_person
                                           ) .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/participantWithTicket
mtparty:participantWithTicket rdf:type owl:ObjectProperty ,
                                       owl:InverseFunctionalProperty ;
                              rdfs:domain mtparty:PublicTVParty ;
                              rdfs:range mtparty:ParticipantWithTicket .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/participant_in
mtparty:participant_in rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/paysEntrance
mtparty:paysEntrance rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain mtparty:PublicParticipant ;
                     rdfs:range gr:PriceSpecification .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/physicalPlace
mtparty:physicalPlace rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf mtparty:location ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:range schema:Place ;
                      owl:propertyDisjointWith mtparty:virtualPlace .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/pws_projector
mtparty:pws_projector rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain mtparty:ProjectorWithScreen ;
                      rdfs:range mtparty:Projector .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/pws_screen
mtparty:pws_screen rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain mtparty:ProjectorWithScreen ;
                   rdfs:range mtparty:ScreenProjector .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/pwt_person
mtparty:pwt_person rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ,
                            owl:InverseFunctionalProperty ;
                   rdfs:domain mtparty:ParticipantWithTicket ;
                   rdfs:range foaf:Person .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/pwt_ticket
mtparty:pwt_ticket rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ,
                            owl:InverseFunctionalProperty ;
                   rdfs:domain mtparty:ParticipantWithTicket ;
                   rdfs:range mtparty:Ticket .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/reaction
mtparty:reaction rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf event:product ;
                 owl:inverseOf mtparty:reaction_in ;
                 rdf:type owl:InverseFunctionalProperty ;
                 rdfs:range mtparty:Reaction .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/reaction_in
mtparty:reaction_in rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/relatedToPublicTVParty
mtparty:relatedToPublicTVParty rdf:type owl:ObjectProperty ,
                                        owl:FunctionalProperty ,
                                        owl:InverseFunctionalProperty ;
                               rdfs:domain mtparty:TicketModel ;
                               rdfs:range mtparty:PublicTVParty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/seller
mtparty:seller rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf event:agent ;
               owl:inverseOf mtparty:seller_in ;
               rdfs:domain mtparty:PublicTVParty ;
               rdfs:range mtparty:Seller .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/seller_in
mtparty:seller_in rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/sellsFood
mtparty:sellsFood rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gr:offers ;
                  rdf:type owl:InverseFunctionalProperty ;
                  rdfs:domain mtparty:FoodSeller ;
                  rdfs:range mtparty:FoodOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/sellsTicket
mtparty:sellsTicket rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf gr:offers ;
                    rdf:type owl:InverseFunctionalProperty ;
                    rdfs:domain mtparty:TicketSeller ;
                    rdfs:range mtparty:SellerTicketOffering .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/startsShowAt
mtparty:startsShowAt rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf time:hasTime ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain mtparty:TVPartyShowSequenceItem .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/startsTVPartyAt
mtparty:startsTVPartyAt rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf time:hasTime ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain mtparty:TVParty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/ticketseller
mtparty:ticketseller rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf mtparty:seller ;
                     owl:inverseOf mtparty:ticketseller_in ;
                     rdfs:range mtparty:TicketSeller .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/ticketseller_in
mtparty:ticketseller_in rdf:type owl:ObjectProperty ;
                        owl:propertyChainAxiom ( mtparty:sellsTicket
                                                 mtparty:includesTicket
                                                 mtparty:relatedToPublicTVParty
                                               ) .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/tvPartyShowSequence
mtparty:tvPartyShowSequence rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf event:factor ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:range mtparty:TVPartyShowSequence .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/virtualPlace
mtparty:virtualPlace rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf mtparty:location ;
                     rdf:type owl:FunctionalProperty ,
                              owl:InverseFunctionalProperty ;
                     rdfs:range schema:VirtualLocation .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#_1
rdf:_1 rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf rdfs:member .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#_2
rdf:_2 rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf rdfs:member .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#_3
rdf:_3 rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf rdfs:member .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#_4
rdf:_4 rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf rdfs:member .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#_5
rdf:_5 rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf rdfs:member .


###  http://www.w3.org/2000/01/rdf-schema#member
rdfs:member rdf:type owl:ObjectProperty ;
            rdfs:isDefinedBy "http://www.w3.org/2000/01/rdf-schema" ;
            rdfs:seeAlso "https://www.w3.org/TR/rdf-schema/#ch_member" .


#################################################################
#    Data properties
#################################################################

###  http://schema.org/genre
schema:genre rdf:type owl:DatatypeProperty .


###  http://schema.org/url
schema:url rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/comment
mtparty:comment rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain mtparty:Reaction ;
                rdfs:range xsd:string ;
                rdfs:comment "This property specifies the comment of review of participant in TV party." .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/password
mtparty:password rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain mtparty:VirtualTicket ;
                 rdfs:range xsd:string ;
                 rdfs:comment "This property specifies the one of two credential (password) that is associated one ticket for access a public virtual TV Party." .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/rating
mtparty:rating rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain mtparty:Reaction ;
               rdfs:range mtparty:ratingInteger ;
               rdfs:comment "This property specifies the number rating (1 to 10) of review of participant in TV party." .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/row
mtparty:row rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain mtparty:PhysicalTicket ;
            rdfs:range xsd:string ;
            rdfs:comment """This property specifies one of the two coordinates (row) where the participant will have to sit.
This property is associated with a ticket to access a public physical TV party.""" .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/runtimeMinutes
mtparty:runtimeMinutes rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain mtparty:Show ;
                       rdfs:range xsd:positiveInteger ;
                       rdfs:comment "This property specifies the duration of the Show (movie, episode) in minutes." .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/seat
mtparty:seat rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain mtparty:PhysicalTicket ;
             rdfs:range xsd:positiveInteger ;
             rdfs:comment """This property specifies one of the two coordinates (seat) where the participant will have to sit.
This property is associated with a ticket to access a public physical TV party.""" .


###  http://www.semanticweb.org/marica/ontologies/marathontvparty/username
